    "temporal_emb": true,
    "spatial_emb": true,
    "activation": "GLU",
    "module_type": "individual",
    "//": ["individual", "sharing"],

    "train_loss": "mse",
    "clip": 10,
    "epochs": 500,
    "seed": 54321,
    "log_interval": 2000,
    "optim": "adam",
    "lr": 0.000001,
    "patience": 30,
    "lr_decay": true,
    "lr_scheduler": "multisteplr",
    "lr_decay_ratio": 0.8,
    "lr_decay_steps":  [5, 10,20,30,40,50],
    "step_size": 10,
    "model_cache_file": "cache/evaluate_cache/best.pt"
  }
  