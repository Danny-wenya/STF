{
    "device": "cuda:0",
    "gpu": true,
    "gpu_id": 1,
    "save": true,
    "cuda": true,
  
    "normalize": 0,
    "window": 12,
    "horizon": 1,
    "batch_size": 32,
    "order": 1,
    "lag": 12,
    "period": 180,
    "sparsity": 0.01,
  
    "hidden_dims": [[64,64,64], [64,64,64], [64,64,64]],  
    "first_layer_embedding_size": 64,  
